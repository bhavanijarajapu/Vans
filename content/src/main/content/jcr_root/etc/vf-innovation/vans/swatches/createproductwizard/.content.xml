<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            jcr:primaryType="nt:unstructured"
            jcr:title="Create a New Swatch "
            sling:resourceType="granite/ui/components/foundation/page">
        <head jcr:primaryType="nt:unstructured">
            <viewport
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/admin/page/viewport"/>
            <meta
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/meta"
                    content="chrome=1"
                    name="X-UA-Compatible"/>
            <favicon
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="cq/gui/components/common/favicon"/>
            <clientlibs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                    categories="[coralui2,granite.ui.foundation,granite.ui.foundation.admin,commerce.gui.orders.admin,cq.authoring.dialog]"/>
        </head>
        <body
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/page/body"
                class="coral--light">
            <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fit"/>
            <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/form"
                    action="${requestPathInfo.suffix}/"
                    currentId="cq-commerce-vans-swatches"
                    rel="cq-commerce-orders-createorder"
                    class="cq-commerce-order-properties"
                    method="post">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/wizard"
                        formMode="edit"/>
                <items jcr:primaryType="nt:unstructured">
                    <create-product-step
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Create New Swatch"
                            sling:resourceType="granite/ui/components/foundation/form/wizard/step">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedColumnContainer
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/container">
                                <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <product-properties
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <scaffold
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="commerce/gui/components/admin/common/includescaffold"/>
                                            <charset
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                    name="_charset_"
                                                    value="utf-8"/>
                                            <node-type
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./jcr:primaryType"
                                                    sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                    value="nt:unstructured"/>
                                            <commerce-type
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./cq:commerceType"
                                                    sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                    value="product"/>
                                            <resource-type
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./sling:resourceType"
                                                    sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                    value="commerce/components/product"/>
                                            <taggable
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./jcr:mixinTypes"
                                                    sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                    value="cq:Taggable"/>
                                            <taggable-typehint
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./jcr:mixinTypes@TypeHint"
                                                    sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                    value="String[]"/>
                                        </items>
                                    </product-properties>
                                </items>
                            </fixedColumnContainer>
                            <wizard-properties
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="commerce/gui/components/admin/common/wizardproperties"
                                    destPath="${requestPathInfo.suffix}"
                                    redirect="/etc/vf-innovation/vans/swatches.html${granite:encodeURIPath(requestPathInfo.suffix)}"/>
                            <cancel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/hyperlink"
                                    class="foundation-wizard-control"
                                    foundation-wizard-control-action="cancel"
                                    href="/etc/vf-innovation/vans/swatches.html/etc/commerce/products/vf-innovation/vans/en_us/swatches"
                                    text="Cancel"/>
                            <create
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/submit"
                                    class="foundation-wizard-control cq-social-edit-submit"
                                    id="cq-social-edit-submit"
                                    disabled="{Boolean}true"
                                    foundation-wizard-control-action="next"
                                    text="Create"/>
                        </items>
                    </create-product-step>
                </items>
            </content>
        </body>
    </jcr:content>
</jcr:root>
